	public static void ipHasha(String[][] countryHash) {
		System.out.println(countryHash.length);
		HashMap<String, String> ipMap = new HashMap<>();
		for (int i = 0; i < countryHash.length; i++) {
			for (int j = 0; j < 1; j++) {
				ipMap.put(countryHash[i][1], countryHash[i][j]);
			}
		}
		System.out.println(ipMap.toString());
	}
